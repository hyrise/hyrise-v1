/* Test merging of tables through MergeTable operator */
{
  "operators": {
    "load_reference": {
      "type": "TableLoad",
      "table": "reference",
      "filename": "tables/employees_revised.tbl"
    },
    "noop" : {
      "type" : "NoOp"
    },
    "load_main" : {
      "type" : "TableLoad",
      "table": "main",
      "filename": "tables/employees.tbl"
    },
    "load_delta" : {
      "type": "TableLoad",
      "table": "delta",
      "filename": "tables/employees_new_row.tbl"
    },
    "merge": {
      "type" : "MergeTable"
    }
  }, 
  "edges": [["load_reference", "noop"], 
            ["noop", "load_main"], 
            ["noop", "load_delta"], 
            ["load_main", "merge"], 
            ["load_delta", "merge"]]
}
