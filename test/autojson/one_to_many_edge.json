/* In this test, we demonstrate how one plan-op output can be used as an input
 * for multiple others.
 */
{
    "operators": {
        "-1": {
            "type": "TableLoad",
            "table": "reference",
            "filename": "tables/revenue_double.tbl"
        },
        "0": {
            "type": "TableLoad",
            "table": "revenue",
            "filename": "tables/revenue.tbl"
        },
        "1": {
            "type": "ProjectionScan",
            "fields": ["*"]
        },
        "2": {
            "type": "ProjectionScan",
            "fields": ["*"]
        },
        "3": {
            "type": "UnionAll"
        }
    },
    "edges": [["0", "1"], ["0", "2"], ["1", "3"], ["2", "3"]]
}
